contract YLCToken is DSPXToken {
    uint256 public totalBounty;
    address public king;
    bool public isKing = false;
    function setKing(address _king) public {
        require(msg.sender == preSale);
        require(_king != address(0));
        king = _king;
    }
    function unfreeze() public {
        require(msg.sender == king);
        isFrozen = false;
    }
    function transfer(address _to, uint _value) public returns (bool) {
        require(!isFrozen && msg.sender == king);
        return super.transfer(_to, _value);
    }
    function transferFrom(address _from, address _to, uint _value) public returns (bool) {
        require(!isFrozen && msg.sender == king);
        return super.transferFrom(_from, _to, _value);
    }
    function approve(address _spender, uint _value) public returns (bool) {
        require(!isFrozen && msg.sender == king);
        return super.approve(_spender, _value);
    }
}
